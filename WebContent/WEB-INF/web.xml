<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>SmartRecruiter</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>GenerateURL</servlet-name>
		<servlet-class>smartRecruiter.GenerateURL</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VerifyToken</servlet-name>
		<servlet-class>smartRecruiter.VerifyToken</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StartTest</servlet-name>
		<servlet-class>smartRecruiter.StartTest</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SubmitTest</servlet-name>
		<servlet-class>smartRecruiter.SubmitTest</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>smartRecruiter.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddJob</servlet-name>
		<servlet-class>smartRecruiter.AddJobProfile</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetCompletedTests</servlet-name>
		<servlet-class>smartRecruiter.GetCompletedTests</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>smartRecruiter.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GenerateURL</servlet-name>
		<url-pattern>/generateURL</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VerifyToken</servlet-name>
		<url-pattern>/verifyToken</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StartTest</servlet-name>
		<url-pattern>/startTest</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SubmitTest</servlet-name>
		<url-pattern>/submitTest</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/loginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddJob</servlet-name>
		<url-pattern>/addJob</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetCompletedTests</servlet-name>
		<url-pattern>/getCompletedTests</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
</web-app>